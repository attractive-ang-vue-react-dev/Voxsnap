{
  "env": {
    "browser": true,
    "es6": true
  },
  "parser": "babel-eslint",
  "plugins": [
    "react"
  ],
  "rules": {
    "accessor-pairs": 2,
    "block-scoped-var": 2,
    "block-spacing": [
      2,
      "always"
    ],
    "brace-style": [
      2,
      "1tbs"
    ],
    "camelcase": [
      2,
      {
        "properties": "always"
      }
    ],
    "comma-dangle": 2,
    "comma-spacing": [
      2,
      {
        "after": true,
        "before": false
      }
    ],
    "computed-property-spacing": [
      2,
      "never"
    ],
    "consistent-this": [
      2,
      "self"
    ],
    "curly": 2,
    "dot-location": [
      2,
      "property"
    ],
    "eqeqeq": 2,
    "guard-for-in": 2,
    "indent": [
      2,
      4,
      {
        "SwitchCase": 1
      }
    ],
    "jsx-quotes": 1,
    "key-spacing": 2,
    "new-cap": 2,
    "new-parens": 2,
    "no-caller": 2,
    "no-cond-assign": 2,
    "no-console": 1,
    "no-constant-condition": 2,
    "no-control-regex": 2,
    "no-debugger": 1,
    "no-dupe-args": 2,
    "no-dupe-keys": 2,
    "no-duplicate-case": 2,
    "no-else-return": 0,
    "no-empty": 2,
    "no-empty-character-class": 2,
    "no-empty-pattern": 2,
    "no-eq-null": 2,
    "no-eval": 2,
    "no-ex-assign": 2,
    "no-extend-native": 2,
    "no-extra-bind": 2,
    "no-extra-boolean-cast": 2,
    "no-extra-semi": 2,
    "no-fallthrough": 2,
    "no-floating-decimal": 2,
    "no-func-assign": 2,
    "no-implicit-coercion": 2,
    "no-implied-eval": 2,
    "no-inner-declarations": 2,
    "no-invalid-regexp": 2,
    "no-invalid-this": 2,
    "no-irregular-whitespace": 2,
    "no-iterator": 2,
    "no-lone-blocks": 2,
    "no-magic-numbers": [
      1,
      {
        "ignore": [
          0,
          1,
          -1
        ],
        "ignoreArrayIndexes": true
      }
    ],
    "no-mixed-spaces-and-tabs": 2,
    "no-multi-spaces": 2,
    "no-multi-str": 2,
    "no-multiple-empty-lines": 2,
    "no-native-reassign": 2,
    "no-negated-in-lhs": 2,
    "no-new-wrappers": 2,
    "no-obj-calls": 2,
    "no-redeclare": 2,
    "no-regex-spaces": 2,
    "no-script-url": 2,
    "no-sequences": 2,
    "no-shadow": 2,
    "no-spaced-func": 2,
    "no-sparse-arrays": 2,
    "no-throw-literal": 2,
    "no-undef": 2,
    "no-undef-init": 2,
    "no-unexpected-multiline": 2,
    "no-unreachable": 2,
    "no-unused-expressions": 2,
    "no-unused-vars": 2,
    "no-use-before-define": 0,
    "no-var": 2,
    "no-void": 2,
    "padded-blocks": [
      2,
      "never"
    ],
    "prefer-arrow-callback": 2,
    "quotes": [
      2,
      "single",
      "avoid-escape"
    ],
    "radix": 2,
    "react/jsx-uses-react": "error",
    "react/jsx-uses-vars": "error",
    "semi": [
      "error",
      "always",
      {
        "omitLastInOneLineBlock": true
      }
    ],
    "space-before-blocks": 2,
    "space-before-function-paren": [
      2,
      "never"
    ],
    "use-isnan": 2,
    "valid-jsdoc": 2,
    "valid-typeof": 2,
    "vars-on-top": 2
  }
}
